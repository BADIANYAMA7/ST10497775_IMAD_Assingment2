<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backiee_291666_portrait"
    tools:context=".Quizz3">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="645dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="Information Technology"
        android:textColor="@color/black"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Score3"
        android:layout_width="159dp"
        android:layout_height="64dp"
        android:layout_marginStart="236dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="581dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="Score:"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/Question3"
        android:layout_width="303dp"
        android:layout_height="210dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="371dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Score3"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/Answer3"
        android:layout_width="307dp"
        android:layout_height="106dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="265dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="User Input (from Right to Left)"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Question3"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/TrueButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="256dp"
        android:layout_marginBottom="212dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="TRUE"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Answer3"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/Result3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="163dp"
        android:layout_marginBottom="211dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="Results"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TrueButton3"
        app:layout_constraintTop_toBottomOf="@+id/Answer3"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/FalseButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="210dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="FALSE"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Result3"
        app:layout_constraintTop_toBottomOf="@+id/Answer3"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/QuestionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginEnd="159dp"
        android:layout_marginBottom="91dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="NEXT"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/Start3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="161dp"
        android:layout_marginBottom="16dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="Start"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/QuestionButton3" />

    <Button
        android:id="@+id/BackToMenu3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="50dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="Menu"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/QuestionButton3"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/feedback3"
        android:layout_width="281dp"
        android:layout_height="72dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="48dp"
        android:background="@android:drawable/alert_light_frame"
        android:text="TextView"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/QuestionButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Answer3"
        tools:visibility="invisible" />
</androidx.constraintlayout.widget.ConstraintLayout>